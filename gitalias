[alias]
        c = checkout
        cc = checkout --
        cb = checkout -b
        po = pull origin
        pod = pull origin develop
        com = checkout master
        cod = checkout develop
        psu = push -u origin
        pfo = push -f origin
        rv = remote -v
        aa = add .
        cm = commit -m
        cmd = commit --amend
        cmdn = commit --amend --no-edit
        ffp = flow feature publish
        fhp = flow hotfix publish
        br = branch
        brd = branch -D
        brm = branch -m
        s = status
        l = log
        cpk = cherry-pick
        cpkc = cherry-pick --continue
        cpka = cherry-pick --abort
        rb = rebase
        rba = rebase --abort
        rbc = rebase --continue
        lg = log --graph --oneline
        st = stash
        stp = stash pop
        stl = stash list
        sh = show
        df = diff
        df1 = diff HEAD~1
        f = fetch
        rsh = reset --hard                                                                                                                                                                         
        rss = reset --soft
        rsh1 = reset --hard HEAD~1                                                                                                                                                                 
        rss1 = reset --soft HEAD~1
