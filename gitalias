[alias]
        pu = pull upstream
        po = pull origin
        fhp = flow hotfix publish
        ffp = flow feature publish
        cm = commit -m
        cmd = commit --amend
        cmdn = commit --amend --no-edit
        c = checkout
        cb = checkout -b
        cc = checkout --
        s = status
        l = log
        a = add
        pfo = push -f origin
        lg = log --graph --oneline -30
        lga = log --graph --oneline
        br = branch
        brd = branch -D
        brm = branch -m
        aa = add .
        rv = remote -v
        stl = stash list
        stp = stash pop
        st = stash
        sh = show
        df = diff
        df1 = diff HEAD~1
        cpk = cherry-pick
        cpka = cherry-pick --abort
        cpkc = cherry-pick --continue
        rb = rebase
        rbc = rebase --continue
        rba = rebase --abort
        f = fetch
        rsh = reset --hard
        rss = reset --soft
        rsh1 = reset --hard HEAD~1
        rss1 = reset --soft HEAD~1
